{
  "name": "doc-tree",
  "description": "Documentation generating tool for JavaScript that matches comments to AST nodes.",
  "version": "0.13.0",
  "license": "GPL-3.0",
  "homepage": "https://github.com/sapeien/doc-tree",
  "repository": {
    "type": "git",
    "url": "git@github.com:sapeien/doc-tree.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "eslint lib test && node test",
    "postpublish": "git tag `npm v doc-tree version` && git push origin --tags"
  },
  "dependencies": {
    "acorn": "^8.4.1",
    "acorn-walk": "^8.1.1",
    "commonmark": "^0.29.3",
    "doctrine": "^3.0.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-boss": "^1.0.6",
    "tape": "^4.14.0"
  },
  "eslintConfig": {
    "extends": "boss",
    "rules": {
      "strict": 0
    }
  },
  "files": [
    "lib/",
    "bin/",
    "LICENSE"
  ],
  "bin": {
    "doc-tree": "bin/doc-tree"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "doc",
    "docs",
    "generator",
    "ast",
    "tool",
    "jsdoc",
    "document",
    "comment",
    "json",
    "documentation"
  ]
}
